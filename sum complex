#include <stdio.h>

typedef struct {
	float real;
	float im;
}complex_t;

void function(complex_t c1, complex_t c2, complex_t* sum) {
	//(*sum).real = c1.real + c2.real;
	//(*sum).im = c1.im + c2.im;
	sum->real = c1.real + c2.real;
	sum->im = c1.im + c2.im;
}

int main(void) {
	
	complex_t c1,c2,sum;
	c1.real = 5;
	c1.im = 6;
	c2.real = 2;
	c2.im = 3;
	function(c1, c2, &sum);
	printf("%f + %fi", sum.real, sum.im);

	return 0;
}
